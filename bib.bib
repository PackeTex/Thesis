@article{Elsholtz_2007,
    author = {Edel, Yves and Elsholtz, Christian and Geroldinger, Alfred and Kubertin, Silke and Rackham, Laurence},
    title = "{Zero-Sum Problems in Finite Abelian Groups and Affine Caps}",
    journal = {The Quarterly Journal of Mathematics},
    volume = {58},
    number = {2},
    pages = {159-186},
    year = {2007},
    month = {05},
    abstract = "{For a finite abelian group G, let s (G) denote the smallest integer l such that every sequence S over G of length | S| ≥ l has a zero-sum subsequence of length exp (G). We derive new upper and lower bounds for s (G), and all our bounds are sharp for special types of groups. The results are not restricted to groups G of the form G=Cnr, but they respect the structure of the group. In particular, we show s(Cn4)≥20n−19 for all odd n, which is sharp if n is a power of 3. Moreover, we investigate the relationship between extremal sequences and maximal caps in finite geometry.}",
    issn = {0033-5606},
    doi = {10.1093/qmath/ham003},
    url = {https://doi.org/10.1093/qmath/ham003},
    eprint = {https://academic.oup.com/qjmath/article-pdf/58/2/159/4523220/ham003.pdf},
}
